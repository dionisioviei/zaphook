#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dionisio/dev/zaphook/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/typescript@4.6.3/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/node_modules:/home/dionisio/dev/zaphook/node_modules:/home/dionisio/dev/node_modules:/home/dionisio/node_modules:/home/node_modules:/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/ts-node@10.7.0_typescript@4.6.3/node_modules/ts-node/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/ts-node@10.7.0_typescript@4.6.3/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/dionisio/dev/zaphook/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/bin/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/typescript@4.6.3/node_modules/typescript/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/typescript@4.6.3/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/node_modules:/home/dionisio/dev/zaphook/node_modules:/home/dionisio/dev/node_modules:/home/dionisio/node_modules:/home/node_modules:/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/ts-node@10.7.0_typescript@4.6.3/node_modules/ts-node/node_modules:/home/dionisio/dev/zaphook/node_modules/.pnpm/ts-node@10.7.0_typescript@4.6.3/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.6.3/node_modules/typescript/bin/tsserver" "$@"
fi
